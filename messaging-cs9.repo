[centos-rabbitmq-38]
name=CentOS-9 - RabbitMQ 38
#baseurl=http://mirror.centos.org/centos/$releasever-stream/messaging/$basearch/rabbitmq-38
metalink=https://mirrors.centos.org/metalink?repo=centos-messaging-sig-rabbitmq-38-$releasever-stream&arch=$basearch&protocol=https,http
# TODO(amoralej) There is some issue with gpg signature for messaging in CS9, disabling it temporarily
gpgcheck=0
enabled=1
module_hotfixes=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-SIG-Messaging

